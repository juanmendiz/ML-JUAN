import pickle

# Objeto a serializar
datos = {'nombre': 'John', 'edad': 30, 'ciudad': 'Nueva York'}

# Serializar objeto y guardarlo en un archivo
with open('datos.pickle', 'wb') as archivo:
    pickle.dump(datos, archivo)

# Deserializar objeto desde el archivo
with open('datos.pickle', 'rb') as archivo:
    datos_deserializados = pickle.load(archivo)

# Imprimir el objeto deserializado
print(datos_deserializados)